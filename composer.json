{
    "name": "hauerheinrich/hh-powermail-mosparo",
    "type": "typo3-cms-extension",
    "description": "Additional field for EXT:powermail. Adds fields for usage of the 'mosparo.io' system.",
    "authors": [
        {
            "name": "Christian Hackl",
            "email": "web@hauer-heinrich.de",
            "homepage": "http://www.hauer-heinrich.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Hauer-Heinrich/hh_powermail_mosparo/issues",
        "source": "https://github.com/Hauer-Heinrich/hh_powermail_mosparo",
        "docs": "https://github.com/Hauer-Heinrich/hh_powermail_mosparo/blob/master/README.md"
    },
    "license": ["GPL-2.0-or-later"],
    "keywords": [
        "TYPO3 CMS",
        "extension",
        "hauer-heinrich"
    ],
    "replace": {
        "hauer-heinrich/hh_powermail_mosparo": "self.version",
        "typo3-ter/hh-powermail-mosparo": "self.version"
    },
    "require": {
        "php": ">=8.2.0",
        "typo3/cms-core": ">= 12.4.0 <= 13.4.99",
        "typo3/cms-backend": ">= 12.4.0 <= 13.4.99",
        "typo3/cms-fluid": ">= 12.4.0 <= 13.4.99",
        "typo3/cms-extbase": ">= 12.4.0 <= 13.4.99",
        "in2code/powermail": ">= 8.0.0 <= 13.0.99"
    },
    "autoload": {
        "psr-4": {
            "HauerHeinrich\\HhPowermailMosparo\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "hh_powermail_mosparo"
        }
    }
}
